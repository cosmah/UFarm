  fieldset 
    table.table.table-bordered
      thead
        tr
          th First Name
          th Last Name
          th Unique No.
          th Ward
          th Contact 
          th Email
          th D.O.B
          th NIN
          th Residence
          th Status
          th Action 
      tbody
        if farmerones
          each farmerone in farmerones
            tr
              td #{farmerone.firstname}
              td #{farmerone.lastname}
              td #{farmerone.uniquenumber}
              td #{farmerone.ward}
              td #{farmerone.phone}
              td #{farmerone.email}
              td #{farmerone.dob}
              td #{farmerone.nin}
              td #{farmerone.residence}
              td #{farmerone.status}
